<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXHamburger?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            prefHeight="799.0" prefWidth="706.0" styleClass="background" stylesheets="@../css/Theme.css"
            xmlns="http://javafx.com/javafx/8" fx:controller="app.RootController">
    <bottom>
        <Label fx:id="infoLabel" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
               minHeight="-Infinity" minWidth="-Infinity" text="BOnTime is ready" textFill="BLUE"
               BorderPane.alignment="CENTER">
            <styleClass>
                <String fx:value="label-bright"/>
                <String fx:value="background"/>
            </styleClass>
        </Label>
    </bottom>
    <top>
        <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
            <left>
                <JFXHamburger fx:id="previousHamburger" style="-fx-cursor: hand;" styleClass="jfx-hamburger-icon"
                              BorderPane.alignment="CENTER"/>
            </left>
            <right>
                <JFXHamburger fx:id="closeHamburger" onMouseClicked="#closeButtonAction" prefHeight="34.0"
                              prefWidth="31.0" styleClass="jfx-hamburger-icon" BorderPane.alignment="CENTER"/>
            </right>
        </BorderPane>
    </top>
</BorderPane>
